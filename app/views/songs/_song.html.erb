<div id="<%= dom_id song %>">
  <p>
    <strong>Title:</strong>
    <%= song.title %>
  </p>

  <p>
    <strong>Filename:</strong>
    <%= song.filename %>
  </p>

  <p>
    <strong>Duration seconds:</strong>
    <%= song.duration_seconds %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= song.year %>
  </p>

  <p>
    <strong>Play count:</strong>
    <%= song.play_count %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= song.notes %>
  </p>

</div>
